@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <div id="Woodworking" style="max-height: 500px;" class="carousel slide overflow-hidden" data-bs-ride="carousel">
        <div class="carousel-inner mx-auto">
            <div class="carousel-item active center-img">
                <img src="/images/Guesthouse.jpg" class="d-block w-100 responsive" alt="...">
            </div class=" center-img">
            <div class="carousel-item justify-content-center">
                <img src="/images/master.jpg" class="d-block w-100 responsive" alt="...">
            </div class=" center-img">
            <div class="carousel-item">
                <img src="/images/living.jpg" class="d-block w-100 responsive" alt="...">
            </div class=" center-img">
            <div class="carousel-item">
                <img src="/images/otherMaster.jpg" class="d-block w-100 responsive" alt="...">
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#Woodworking"
            data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#Woodworking"
            data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
</div>
